{
  "name": "bot",
  "version": "1.1.0",
  "description": "A template for Discord.js v13 written in Typescript",
  "main": "./build/bot.ts",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "custom-types": "*",
    "discord-api-types": "^0.26.0",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "js-yaml": "^4.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.18",
    "@types/sqlite3": "3.1.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "8.6.0",
    "jest": "27.4.5",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.4"
  },
  "scripts": {
    "dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/bot.ts",
