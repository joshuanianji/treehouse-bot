{
  "name": "bot",
  "version": "1.1.0",
  "description": "A template for Discord.js v13 written in Typescript",
  "main": "./build/bot.ts",
  "dependencies": {
    "@discordjs/rest": "1.7.1",
    "axios": "0.27.2",
    "chalk": "4.1.2",
    "custom-types": "*",
    "discord-api-types": "0.37.43",
    "discord.js": "13.16.0",
    "fp-ts": "2.16.0",
    "io-ts": "2.2.20",
    "io-ts-reporters": "2.0.1",
    "js-yaml": "4.1.0",
    "utils": "*"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/ms": "0.7.31",
    "@types/node": "18.16.16",
    "@types/sqlite3": "3.1.8",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.42.0",
    "jest": "27.5.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.9.5"
  },
  "scripts": {
    "dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/bot.ts",
    "build": "ncc build src/bot.ts --minify --out build",
    "start": "node build/bot.js",
    "lint": "eslint --ext .ts,.js ./src/",
    "lint:fix": "eslint --ext .ts,.js ./src/ --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingOKarma/typescript-discord-bot-template.git"
  },
  "author": "KingOKarma",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/KingOKarma/typescript-discord-bot-template/issues"
  },
  "homepage": "https://github.com/KingOKarma/typescript-discord-bot-template#readme"
}
