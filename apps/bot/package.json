{
  "name": "bot",
  "version": "1.1.0",
  "description": "A template for Discord.js v13 written in Typescript",
  "main": "./build/bot.ts",
  "dependencies": {
    "@discordjs/rest": "^0.2.0-canary.0",
    "axios": "^0.25.0",
    "chalk": "^4.1.2",
    "custom-types": "*",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "fp-ts": "^2.11.6",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.0",
    "js-yaml": "^4.0.0",
    "next": "12.0.8",
    "utils": "*"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.21",
    "@types/sqlite3": "3.1.8",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "@vercel/ncc": "0.33.1",
    "eslint": "8.7.0",
    "jest": "27.4.7",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.4"
  },
  "scripts": {
    "dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/bot.ts",
    "build": "ncc build src/bot.ts --minify --out build",
    "start": "node build/bot.js",
    "lint": "eslint --ext .ts,.js ./src/",
    "lint:fix": "eslint --ext .ts,.js ./src/ --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingOKarma/typescript-discord-bot-template.git"
  },
  "author": "KingOKarma",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/KingOKarma/typescript-discord-bot-template/issues"
  },
  "homepage": "https://github.com/KingOKarma/typescript-discord-bot-template#readme"
}
